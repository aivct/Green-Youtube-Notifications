{
  "manifest_version": 2,
  "name": "Green Notifications for Youtube",
  "description": "Makes Youtube web app notifications green.",
  "version": "0.0.1",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["scripts/green.js"],
      "run_at": "document_idle"
    }
  ],
  "icons":{
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  }
}